package com.syntax.class34;

public class ThrowsExample {
public static void main(String[] args)  {
	multplesleep();
}

public static void  multplesleep()  {
	try {
		sleep();
	} catch (InterruptedException e) {
	
		e.printStackTrace();
	} // is being delegated 
}

public static void sleep() throws InterruptedException {
	Thread.sleep(2000);
}
}
