package Arrays;

import java.util.Scanner;

public class Arrays_Part_One {
  public static void main(String[]args) {
	  
	  Scanner keyboard= new Scanner(System.in);
	  System.out.println("Entre the size of the arrays ");
	  
	  System.out.println("Entre Size of arrays ");
	  int size;
	  size=keyboard.nextInt();
	  double[]salaries=new double[size];
	   int i;
	   for(i=0;i<salaries.length;i++)      // ?
	   {
		   System.out.println("Entre salary for Employee No"+(i+1));
		   salaries[i]=keyboard.nextDouble();    // ?
		   
		   
		   
	   }
	  
	  
	  
	  
	  
	  
	  
  }
}
